{
  "manifest_version": 3,
  "name": "Tailwind CSS JIT in-browser compiler",
  "version": "3.2.4",
  "description": "Locally injected version of https://cdn.tailwindcss.com/ to be used in dev to support hmr and JIT classes parsing",
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": ["src/pages/content/index.js"],
      "css": ["assets/css/contentStyle.chunk.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/js/*.js",
        "assets/css/*.css",
        "src/pages/extension/index.js",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": ["*://*/*"]
    }
  ]
}
